    "types": "./lib/types/index.d.ts",
    "import": "./lib/index.js"
  },
  "files": [
    "lib/*",
    "!*.{test,stories}.*"
  ],
  "license": "AGPL-3.0-or-later",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "graphql": "^16.8.1",
    "nexus": "^1.3.0",
    "warn-once": "^0.1.1"
  },
  "devDependencies": {
    "@webstudio-is/tsconfig": "workspace:*",
    "@webstudio-is/sdk": "workspace:*"
  }
